@model List<Experience>
@{
    ViewData["Title"] = "Index";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

<br />
<table class="table table-hover">
    <tr>
        <th>#</th>
        <th> Deneyim</th>
        <th> Tarih</th>
        <th> Güncelle</th>




    </tr>

    @foreach (var item in Model)
    {
<tr>
    <th>@item.ExperienceId</th>
    <td>@item.Title</td>
    <td>@item.Date</td>


    <td><a href="/AdminExperience/ExperienceUpdate/@item.ExperienceId" class="btn btn-outline-warning"><b>Güncelle</b></a></td>



</tr>}

</table>
<br />
<a href="/AdminExperience/AddExperience" class="btn btn-outline-dark"> <b>Deneyim Ekle</b></a>
<br />


